{
  "name": "@ant-design/nextjs-registry",
  "version": "1.3.0",
  "description": "Register of Next.js for Ant Design",
  "keywords": [
    "react",
    "nextjs",
    "antd",
    "ant-design"
  ],
  "license": "MIT",
  "main": "./lib/index",
  "module": "./es/index",
  "files": [
    "es",
    "lib"
  ],
  "homepage": "https://github.com/ant-design/nextjs-registry",
  "repository": {
    "type": "git",
    "url": "git@github.com:ant-design/nextjs-registry.git"
  },
  "bugs": {
    "url": "https://github.com/ant-design/nextjs-registry/issues"
  },
  "scripts": {
    "dev": "next dev example/with-app-router",
    "compile": "father build",
    "lint": "eslint src/ --ext .ts,.tsx,.jsx,.js,.md",
    "lint-staged": "lint-staged",
    "prepare": "husky install",
    "prepublishOnly": "npm run compile && np --yolo --no-publish",
    "prettier": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\""
  },
  "devDependencies": {
    "@ant-design/cssinjs": "^2.0.1",
    "@types/react": "^18.3.27",
    "@types/react-dom": "^18.3.7",
    "antd": "^6.0.0",
    "eslint": "^9.0.0",
    "father": "^4.6.8",
    "husky": "^8.0.3",
    "lint-staged": "^13.3.0",
    "next": "^16.0.4",
    "np": "^7.7.0",
    "prettier": "^2.8.8",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "typescript": "^5.9.3",
    "@antfu/eslint-config": "^6.2.0"
  },
  "peerDependencies": {
    "@ant-design/cssinjs": ">=1.0.0",
    "antd": ">=5.0.0",
    "next": ">=14.0.0",
    "react": ">=16.0.0",
    "react-dom": ">=16.0.0"
  },
  "lint-staged": {
    "**/*.{js,jsx,tsx,ts,md,json}": [
      "prettier --write",
      "git add"
    ]
  }
}
